{
  "rules": {
    "ESLint Rules List": "https://eslint.org/docs/rules/",

    "使用双引号": "single,double",
    "quotes": [2, "double"],

    "语句结束的分号": "never,always",
    "semi": [2, "always"],

    "禁止使用console": "",
    "no-console": 2,

    "禁止修改const声明的变量": "",
    "no-const-assign": 2,

    "在创建对象字面量时不允许键重复 {a:1,a:1}": "",
    "no-dupe-keys": 2,

    "箭头函数的参数用小括号括起来": "",
    "arrow-parens": [2, "always"],

    "switch语句最后必须有default": "",
    "default-case": 2,

    "必须使用全等": "",
    "eqeqeq": 2,

    "缩进风格": "",
    "indent": [2, 2]
  }
}
